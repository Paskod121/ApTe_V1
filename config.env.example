# Configuration de base
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:3000

# Base de données MongoDB
MONGODB_URI=mongodb://localhost:27017/apte

# JWT Secret (à changer en production)
JWT_SECRET=apte-secret-key-2024-change-in-production

# Configuration email (pour la vérification)
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password

# Configuration Google OAuth (optionnel)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# Session secret
SESSION_SECRET=apte-session-secret-change-in-production 